%h1 Me2day service

-form_for(@feedsetting) do |f|
  = f.error_messages
  %p
    %img{:src => "../images/googlereader.png", :alt => "me2day"}
    = "Your Google Reader URL(You can find the URL from Google Reader shared items page)"
    %br/
    = f.text_field :parameter
  %p
    = f.submit "Create"

=link_to 'Back', services_path

